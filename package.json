{
  "name": "great-novels-api",
  "version": "1.0.0",
  "description": "",
  "main": "index",
  "scripts": {
    "dev": "./node_modules/.bin/env-cmd ./node_modules/.bin/nodemon index.js",
    "dev:migrate": "./node_modules/.bin/env-cmd ./node_modules/.bin/sequelize db:migrate --env development --config ./config/sequelize.js",
    "dev:db:create": "./node_modules/.bin/env-cmd ./node_modules/.bin/sequelize db:create --env development --config ./config/sequelize.js",
    "prod:migrate": "./node_modules/.bin/sequelize db:migrate --env production --config ./config/sequelize.js",
    "build": "npm build",
    "start": "npm install && npm run prod:migrate && npm run build && node index.js",
    "prod:package": "npm-pack-zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackeducation/great-novels-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stackeducation/great-novels-api/issues"
  },
  "homepage": "https://github.com/stackeducation/great-novels-api/public/index",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2"
  }
}
